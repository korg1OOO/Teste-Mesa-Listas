$sete_maravilhas[7] = [
"Cristo Redentor",
"Chichén Itzá",
"Coliseu",
"Muralha da China",
"Petra",
"Machu Picchu",
"Taj Mahal"
];

for($i=0; $i < 7; $i++){
  print($i+1, ": ", $sete_maravilhas[$i], "\n");
}

Memória
$i = 0
$i = 1
$i = 2
$i = 3
$i = 4
$i = 5
$i = 6

Console
1: Cristo Redentor
2: Chichén Itzá
3: Coliseu
4: Muralha da China
5: Petra
6: Machu Picchu
